fn main() {
    let mut str = String::from("   Hello, Rust programming!   ");

    let capacity = str.capacity();
    println!("Capacity: {}", capacity);

    let contains_sub = str.contains("Rust");
    println!("Contains 'Rust': {}", contains_sub);

    let replaced = str.replace("Rust", "Rustacean");
    println!("Replaced: {}", replaced);

    let trimmed = str.trim();
    println!("Trimmed: '{}'", trimmed);
}
